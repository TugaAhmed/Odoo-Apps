<odoo>

	<template id='student_template_id'>
		<t t-call="web.html_container">

			<t t-foreach="docs" t-as='student' >
				<h1> Students Report </h1>  <br></br><br></br>

				<b> Name :  </b><text t-field = 'student.name'/>
				<br></br> 

				<b> Gender : </b><text t-field = 'student.gender'/>
			    <br></br>

			    <b> Is Foreigner  </b> <text t-field = 'student.is_foreigner'/>
			    <br></br>  <br></br><br></br>
			</t>
		</t>
	</template>
</odoo>